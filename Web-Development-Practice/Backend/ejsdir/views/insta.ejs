<%- include("includes/head.ejs") %>
<body>
    <h1>This Page Belongs to @<%= data.name %></h1>
    <button>Follow</button>
    <button>Message</button>

    <p>Followers : <%= data.followers%> &nbsp;&nbsp;&nbsp; Following :  <%= data.following%></p><hr>

    <% for(let post of data.posts){ %>
        <img src="<%=post.image%>" alt="Some image">
        <p>Likes : <%= post.likes%> &nbsp;&nbsp;&nbsp; Comments :  <%= post.comments%></p><hr>
    <%}%>
</body>
</html>